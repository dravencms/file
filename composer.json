{
  "name": "dravencms/file",
  "type": "dravencms-package",
  "description": "File module for dravencms",
  "keywords": [
    "dravencms",
    "file"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "salamek/nette-files": "@dev",
    "dravencms/locale": ">=1.0",
    "dravencms/admin": ">=2.0"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "suggest": {
    "dravencms/structure": "Allows file components to be used in cms"
  },
  "extra": {
    "dravencms": {
      "files": {
        "assets/*": "%wwwDir%/assets/file"
      },
      "configuration": "dravencms.config.neon"
    }
  }
}
